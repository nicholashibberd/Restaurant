<h2>Sign in</h2>
<% form_for(:session, :url => sessions_path) do |f| %> 
<%= f.error_messages %>
	<div class="field">
		<%= f.label :email %><br />
		<%= f.text_field :email %>
	</div>
	<div class="field">
		<%= f.label :password %><br /> 
		<%= f.password_field :password %>
	</div> 
	<div class="actions">
		<%= f.submit "Sign in" %> 
	</div>
		<% end %> 
	<p>New user? <%= link_to "Sign up!", signup_path %></p>